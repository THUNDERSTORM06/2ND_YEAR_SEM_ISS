<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
    <class name="AtributiePresent" table="ATRIBUTII_PRESENT">
        <id name="id" column="id"> <generator class="increment"/> </id>
        <property name="voluntar" column="voluntar"/>
        <property name="atributie" column="atributie"/>
        <property name="returned" column="present"/>
        <property name="days" column="days"/>
    </class>
</hibernate-mapping>